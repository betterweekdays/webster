                                {{/bdubs-text1}}

                                {{# bdubs-text2}}
                                    &lt;p&gt;{{ bdubs-text2 }}&lt;/p&gt;
                                {{/bdubs-text2}}

                                {{# bdubs-text3}}
                                    &lt;p&gt;{{ bdubs-text3 }}&lt;/p&gt;
                                {{/bdubs-text3}}
                            &lt;/div&gt;
                        &lt;/div&gt;
                    &lt;/div&gt;

                    {{# bdubs-name-center}}{{&gt;molecules-name-center}}{{/bdubs-name-center}}

                    {{# bdubs-degreeprogram}}
                        &lt;div class=&quot;row text-center&quot;&gt;
                            &lt;form role=&quot;form&quot; class=&quot;form-inline giant&quot; action=&quot;#&quot;&gt;
                                &lt;div class=&quot;form-group&quot;&gt;
                                    &lt;select class=&quot;form-control&quot;&gt;
                                        &lt;option value=&quot;1&quot; selected=&quot;selected&quot;&gt;Accounting&lt;/option&gt;
                                        &lt;option value=&quot;2&quot;&gt;Biology&lt;/option&gt;
                                        &lt;option value=&quot;3&quot;&gt;Business Administration&lt;/option&gt;
                                        &lt;option value=&quot;4&quot;&gt;Elementary Education&lt;/option&gt;
                                        &lt;option value=&quot;5&quot;&gt;Finance&lt;/option&gt;
                                        &lt;option value=&quot;6&quot;&gt;Medical Assisting&lt;/option&gt;
                                    &lt;/select&gt;
                                    &lt;select class=&quot;form-control&quot;&gt;
                                        &lt;option value=&quot;1&quot; selected=&quot;selected&quot;&gt;Anywhere&lt;/option&gt;
                                        &lt;option value=&quot;2&quot;&gt;Chicago, IL&lt;/option&gt;
                                        &lt;option value=&quot;2&quot;&gt;Kansas City, MO&lt;/option&gt;
                                        &lt;option value=&quot;2&quot;&gt;Miama, FL&lt;/option&gt;
                                        &lt;option value=&quot;2&quot;&gt;New York, NY&lt;/option&gt;
                                        &lt;option value=&quot;3&quot;&gt;St. Louis, MO&lt;/option&gt;
                                    &lt;/select&gt;
                                &lt;/div&gt;
                                &lt;button type=&quot;submit&quot; class=&quot;btn btn-success&quot; onclick=&quot;window.location='{{ link.pages-jsj-intro-journey3 }}';&quot;&gt;Send&lt;/button&gt;
                            &lt;/form&gt;
                            &lt;div class=&quot;row small&quot;&gt;
                                &lt;p&gt;(Don't worry. You can change this later!)&lt;/p&gt;
                            &lt;/div&gt;
                        &lt;/div&gt;
                    {{/bdubs-degreeprogram}}

                    {{# bdubs-jobs-now}}
                        &lt;form role=&quot;form&quot; class=&quot;form-inline giant text-center&quot; action=&quot;#&quot;&gt;
                            &lt;button type=&quot;submit&quot; class=&quot;btn btn-info&quot; onclick=&quot;window.location='{{ link.pages-jsj-intro-journey3 }}';&quot;&gt;Not yet. Tell me more.&lt;/button&gt;
                            &lt;button type=&quot;submit&quot; class=&quot;btn btn-success&quot; onclick=&quot;window.location='{{ link.pages-jsj-jobs-journey }}';&quot;&gt;Yes&lt;/button&gt;
                        &lt;/form&gt;
                    {{/bdubs-jobs-now}}

                &lt;/div&gt;
            &lt;/div&gt;
        &lt;/div&gt;
    &lt;/div&gt;
