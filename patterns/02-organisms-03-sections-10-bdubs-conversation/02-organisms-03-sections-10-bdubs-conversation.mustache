&lt;div class=&quot;main-box clearfix&quot;&gt;
    &lt;div class=&quot;main-box-body clearfix&quot;&gt;
        &lt;div class=&quot;conversation-wrapper&quot;&gt;
            &lt;div class=&quot;conversation-content&quot;&gt;
                &lt;div class=&quot;conversation-inner&quot;&gt;

                    {{#bdubs-journey-pts}}
                    &lt;div&gt;
                        &lt;p&gt;Your Journey Points: {{bdubs-journey-pts}}&lt;/p&gt;
                    &lt;/div&gt;
                    &lt;div class=&quot;progress&quot;&gt;
                        &lt;div class=&quot;progress-bar&quot; role=&quot;progressbar&quot; aria-valuenow=&quot;{{bdubs-journey-pts}}&quot; aria-valuemin=&quot;0&quot; aria-valuemax=&quot;100&quot; style=&quot;width: {{bdubs-journey-pts}}%;&quot;&gt;
