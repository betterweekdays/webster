&lt;div class=&quot;col-lg-8&quot;&gt;
    &lt;div class=&quot;main-box&quot;&gt;
        &lt;header class=&quot;main-box-header clearfix&quot;&gt;
            &lt;h2&gt;Before You Begin...&lt;/h2&gt;
        &lt;/header&gt;

        &lt;div class=&quot;main-box-body clearfix&quot;&gt;
            &lt;form role=&quot;form&quot;&gt;
